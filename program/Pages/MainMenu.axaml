<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="program.Pages.MainMenu">
	<StackPanel Grid.Row="1" Grid.ColumnSpan="3" Orientation="Vertical">
		<Label Content="Добро пожаловать!" FontWeight="Bold"/>
	<Button Name="DataBtn" Content="Данные" 
			Click="dataBtnClick"/>
		<Button Name="HelpBtn" Content="Справка"/>
	</StackPanel>
</UserControl>
